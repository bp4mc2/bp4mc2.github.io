  mim:naam "Composite"@nl;
  mim:definitie "Compositie (gesloten wiebertje)"@nl;
.
mim:Aggregate a mim:Enumeratiewaarde;
  rdfs:label "Aggregate"@nl;
  mim:naam "Aggregate"@nl;
